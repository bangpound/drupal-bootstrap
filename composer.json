{
    "name": "bangpound/drupal-bootstrap",
    "description": "Drupal 7 bootstrap replacement",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/drupal",
                "version": "7.26",
                "dist": {
                    "url": "http://ftp.drupal.org/files/projects/drupal-7.26.tar.gz",
                    "type": "tar"
                },
                "source": {
                    "url": "http://git.drupal.org/project/drupal.git",
                    "type": "git",
                    "reference": "7.x-dev"
                },
                "autoload": {
                    "classmap": [
                        "authorize.php", "cron.php", "includes/", "index.php", "install.php", "modules/",
                        "profiles/", "sites/", "themes/", "update.php", "xmlrpc.php"
                    ]
                }
            }
        }
    ],
    "require": {
        "pimple/pimple": "~1.1"
    },
    "require-dev": {
        "drupal/drupal": "7.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Drupal\\Core\\": ""
        }
    },
    "target-dir": "Drupal/Core",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Benjamin Doherty",
            "email": "bjd@bangpound.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}