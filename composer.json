{
    "name": "drufony/drupal-bootstrap",
    "description": "Drupal 7 bootstrap replacement",
    "repositories": [
        {
            "type": "composer",
            "url": "http://drufony.github.io"
        }
    ],
    "require": {
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "drupal/drupal": "dev-7.x-pimple"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Core\\Tests\\": "Tests/"
        }
    },
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Benjamin Doherty",
            "email": "bjd@bangpound.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-develop": "1.2.x-dev"
        }
    }
}
